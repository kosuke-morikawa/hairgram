<h1>Catalogs#index</h1>
<p>Find me in app/views/catalogs/index.html.erb</p>

<% @catalogs.each do |catalog| %>
<%= attachment_image_tag catalog, :image %>
<%= attachment_image_tag catalog.user, :profile_image, fallback: "no-image.png" %>
<%= catalog.user.username %>
<%= catalog.updated_at.strftime("%Y-%m-%d %H:%M") %>
<% end %>
